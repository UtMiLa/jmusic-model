{
  "name": "jmusic-model",
  "version": "0.0.6",
  "description": "Music theory model",
  "main": "dist/index.js",
  "author": "John Ekberg <john@apus.dk>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rimraf dist && ttsc --build tsconfig.prod.json",
    "start": "yarn run start:dev",
    "test": "mocha --config .mocharc.json --watch src/**/*.test.ts",
    "coverage": "nyc mocha --config .mocharc.json src/**/*.test.ts",
    "lint": "eslint src/**/*.ts --fix",
    "serve": "webpack serve --open",
    "start:dev": "nodemon --config .nodemonrc.json src/index.ts",
    "start:prod": "node dist/index.js",
    "prepub": "npm version patch & copy package.json dist",
    "pub": "cd dist & npm publish",
    "postpub": "cd dist & del package.json"
  },
  "dependencies": {
    "deep-eql": "^4.1.2",
    "node": "^15.3.0",
    "ramda": "^0.28.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/deep-eql": "^4.0.0",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.10",
    "@types/ramda": "^0.28.22",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "chai": "^4.2.0",
    "eslint": "^7.14.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.2",
    "ts-loader": "^9.4.4",
    "ts-node": "^9.0.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.1.2",
    "typescript-transform-paths": "^2.1.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  }
}
